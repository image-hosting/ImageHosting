<title id="MathJax-SVG-1-Title">a%2Bb%3D1\\
\begin{aligned}
\mathbf{a} \times \mathbf{b}\overset{分配率}{%3D}
%26\left(a_{1} \mathbf{i}%2Ba_{2} \mathbf{j}%2Ba_{3} \mathbf{k}\right) \times\left(b_{1} \mathbf{i}%2Bb_{2} \mathbf{j}%2Bb_{3} \mathrm{k}\right) \\
%3D%26 a_{1} b_{1}(\mathbf{i} \times \mathbf{i})%2Ba_{1} b_{2}(\mathbf{i} \times \mathbf{j})%2Ba_{1} b_{3}(\mathbf{i} \times \mathbf{k})%2B\\
%26 a_{2} b_{1}(\mathbf{j} \times \mathbf{i})%2Ba_{2} b_{2}(\mathbf{j} \times \mathbf{j})%2Ba_{2} b_{3}(\mathbf{j} \times \mathbf{k})%2B\\
%26 a_{3} b_{1}(\mathbf{k} \times \mathbf{i})%2Ba_{3} b_{2}(\mathbf{k} \times \mathbf{j})%2Ba_{3} b_{3}(\mathbf{k} \times \mathbf{k})\\
%3D%26\left(a_{2} b_{3}-a_{3} b_{2}\right) \mathbf{i}%2B\left(a_{3} b_{1}-a_{1} b_{3}\right) \mathbf{j}%2B\left(a_{1} b_{2}-a_{2} b_{1}\right) \mathbf{k}
\end{aligned}</title>
